<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE XML>
<!-- 꾸미기(레이아웃)에 대한 선언 -->
<decorators defaultdir="/WEB-INF/views/decorator">
	<!-- hello에 대한 요청이 들어오면 적용시키는 레이아웃 선언 -->
	<decorator name="main" page="default_decorator.jsp">
		<pattern>/main/*</pattern>
		<pattern>/error/*</pattern>
		<pattern>/notice/*</pattern>
		<pattern>/event/*</pattern>
		<pattern>/book/*</pattern>
		<pattern>/tour/*</pattern>
		<pattern>/cart/*</pattern>
		<pattern>/member/*</pattern>
		<pattern>/qna/*</pattern>
		<pattern>/siteqna/*</pattern>
		<pattern>/anonymous/*</pattern>
		<pattern>/review/*</pattern>
	</decorator>
	<!-- 게시판의 대한 요청 레이아웃 선언 -->

	<!-- 데코레이션에서 제외되는 요청을 선언 -->
	<excludes>
<!-- 	<pattern>/member/readme.do</pattern> -->
	</excludes>

</decorators>